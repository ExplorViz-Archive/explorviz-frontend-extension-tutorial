{{#if landscapeService.landscape}}
Selected Landscape:{{landscapeService.landscape.timestamp}}
{{else}}
  {{landscapeService.landscapeList.length}} possible Landscapes:
  <ul>
    {{#each landscapeService.landscapeList as |landscape|}}
      <li>
        <a class="dropdown-item d-flex-center" style="cursor: pointer" title="set landscape"  {{action "setTimestamp" model landscape.timestamp.timestamp}}>
          {{landscape.timestamp.name}}
        </a>
      </li>
    {{/each}}
  </ul>
{{/if}}
